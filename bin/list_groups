#!/usr/bin/env python

from roboto.requests.awsqueryrequest import AWSQueryRequest
from boto.iam import IAMConnection

if __name__ == '__main__':
    AWSQueryRequest.JsonPath = '/Users/mitch/Projects/roboto/roboto/services/aws/iam'
    AWSQueryRequest.ConnectionCls = IAMConnection
    r = AWSQueryRequest('ListGroups')
    r.do_cli()
